CREATE TABLE Client (
                          id INT NOT NULL,
                          nume VARCHAR2(255),
                          prenume VARCHAR2(255),
                          age INT,
                          sex VARCHAR2(255),
                          cnp VARCHAR2(255),
                          fidelity VARCHAR2(255),
                          PRIMARY KEY (id)
);

CREATE TABLE PersonalTrainer (
                          id INT NOT NULL,
                          nume VARCHAR2(255),
                          prenume VARCHAR2(255),
                          age INT,
                          sex VARCHAR2(255),
                          calificare VARCHAR2(255),
                          dataAngajarii DATE,
                          specializare VARCHAR2(255),
                          experienta INT,
                          PRIMARY KEY (id)
);

CREATE TABLE Gym (
                        id INT NOT NULL,
                        adresa VARCHAR2(255),
                        numarClienti INT,
                        PRIMARY KEY (id)
);

CREATE TABLE PTtoGYM (
                        idPersonalTrainer INT NOT NULL,
                        idGym INT NOT NULL
);

CREATE TABLE Membership (
                        id INT NOT NULL,
                        startingDate DATE,
                        expirationDate DATE,
                        idClient INT NOT NULL,
                        PRIMARY KEY(id),
                        FOREIGN KEY (idClient) REFERENCES Client(id)
);